{
  "manifest_version": 2,
  "name": "pub-sub",
  "version": "0.01",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "logo_y.png",
    "default_popup": "popup/popup.html",
    "default_title": "my popup"
  }
}